<section class="admin-categories-index">
  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>
  <div class="well">
    <h3>
      <%= link_to "New Category"%>
    </h3>
  </div>
  <div class="panel panel-default categories">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Category</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <%= render "category" %>
      </tbody>
    </table>
  </div>
</section>
